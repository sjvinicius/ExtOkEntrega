    "FULL (RG-HC)": {
        "prefix": "oktrycatchfinally",
        "body": [
            "try {",
            "    //! In case of error in __construct ",
            "    if (\\$this->retorno['resposta_status']['status'] == 1) {",
            "        \\$this->retorno = \\$this->iniciar_retornofuncao();",
            "    }",
            "} catch (\\Exception \\$e) {",
            "    \\$this->retorno(\\$e->getLine(), \\$e->getMessage(), status: \\$e->getCode());",
            "} finally {",
            "    \\$this->retorno['resposta_status']['time'] = microtime(true) - \\$this->retorno['resposta_status']['time'];",
            "}",
            "",
            "return \\$this->retorno;"
        ],
        "description": "try catch finally default"
    },
    "FULL (OK)": {
