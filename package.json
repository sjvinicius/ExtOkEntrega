{
  "name": "extok",
  "displayName": "OK Entrega (Snippets)",
  "description": "Atalhos de padroniação de código do okentrega",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com/sjvinicius/ExtOkEntrega.git"
	},
  "publisher": "ViniciusSilvadeJesus",
  "categories": [
    "Snippets",
    "Other"
  ],
  "icon": "images/logo/oklogo.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "keywords": [
		"Ok",
		"Ok Entrega",
		"Ok ENTREGA",
		"OKENTREGA",
		"OK",
		"ok"
	],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}